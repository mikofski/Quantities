<constant name="speed_of_light" value="299792458*meter/second">
  <alias>c</alias>
</constant>

<constant name="standard_gravity" value="9.806650*meter/second^2">
  <alias>g_0</alias>
  <alias>g_n</alias>
  <alias>gravity</alias>
</constant>

<constant name="vacuum_permeability" value="4*pi*1e-7*newton/ampere^2">
  <alias>mu_0</alias>
  <alias>magnetic_constant</alias>
</constant>

<constant name="vacuum_permittivity" value="1/(mu_0*c^2)">
  <alias>epsilon_0</alias>
  <alias>electric_constant</alias>
</constant>

<constant name="Z_0" value="mu_0*c">
  <alias>impedance_of_free_space</alias>
  <alias>characteristic_impedance_of_vacuum</alias>
</constant>

<constant name="planck_constant" value="6.62606957e-34*J*s">
  <alias>h</alias>
</constant>

<constant name="hbar" value="planck_constant/(2*pi)">
  <alias>ħ</alias>
</constant>

<constant name="newtonian_constant_of_gravitation"
          value="6.67384e-11*m^3*kg^-1*s^-2"/>

<constant name="molar_gas_constant" value="8.3144621*J*mol^-1*K^-1">
  <alias>R</alias>
</constant>

<constant name="fine_structure_constant" value="7.2973525698e-3*dimensionless"/>

<constant name="avogadro_number" value="6.02214129e23*mol^-1">
  <alias>N_A</alias>
</constant>

<constant name="boltzmann_constant" value="1.3806488e-23*J*K^-1">
  <alias>k</alias>
</constant>

<constant name="stefan_boltzmann_constant" value="5.670373e-8*W*m^-2*K^-4">
  <alias>σ</alias>
</constant>

<constant name="wien_frequency_displacement_law_constant"
          value="5.8789254e10*Hz*K^-1"/>

<constant name="rydberg_constant" value="10973731.568539*m^-1"/>

<constant name="electron_mass" value="9.10938291e-31*kg">
  <alias>m_e</alias>
</constant>

<constant name="neutron_mass" value="1.674927351e-27*kg">
  <alias>m_n</alias>
</constant>

<constant name="proton_mass" value="1.672621777e-27*kg">
  <alias>m_p</alias>
</constant>
