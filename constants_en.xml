<constant name="speed_of_light" value="299792458 * meter / second">
  <alias>c</alias>
</constant>
<constant name="standard_gravity" value="9.806650 * meter / second ^ 2">
  <alias>g_0</alias>
  <alias>g_n</alias>
  <alias>gravity</alias>
</constant>
<constant name="vacuum_permeability" value="4 * pi * 1e-7 * newton / ampere ^ 2">
  <alias>mu_0</alias>
  <alias>magnetic_constant</alias>
</constant>
<constant name="vacuum_permittivity" value="1 / (mu_0 * c ^ 2 )">
  <alias>epsilon_0</alias>
  <alias>electric_constant</alias>
</constant>
<constant name="Z_0" value="mu_0 * c">
  <alias>impedance_of_free_space</alias>
  <alias>characteristic_impedance_of_vacuum</alias>
</constant>