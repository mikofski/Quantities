<?xml version="1.0"?>

<!DOCTYPE doc [
<!ENTITY prefixes SYSTEM "prefixes_en.xml">
<!ENTITY dimensions SYSTEM "dimensions_en.xml">
<!ENTITY constants SYSTEM "constants_en.xml">
<!ENTITY angle SYSTEM "angle_en.xml">
<!ENTITY length SYSTEM "length_en.xml">
<!ENTITY area SYSTEM "area_en.xml">
]>

<ureg>

  <!-- prefixes -->
  &prefixes;

  <!-- dimensions -->
  &dimensions;

  <!-- constants -->
  &constants;

  <!-- reference -->
  <unit name="meter" dimensionality="length">
    <alias>m</alias>
    <alias>metre</alias>
  </unit>

  <unit name="second" dimensionality="time">
    <alias>s</alias>
    <alias>sec</alias>
  </unit>

  <unit name="ampere" dimensionality="current">
    <alias>A</alias>
    <alias>amp</alias>
  </unit>

  <unit name="candela" dimensionality="luminosity">
    <alias>cd</alias>
    <alias>candle</alias>
  </unit>

  <unit name="gram" dimensionality="mass">
    <alias>g</alias>
  </unit>

  <unit name="mole" dimensionality="substance">
    <alias>mol</alias>
  </unit>

  <unit name="kelvin" dimensionality="temperature">
    <alias>K</alias>
    <alias>degK</alias>
  </unit>

  <unit name="radian">
    <alias>rad</alias>
  </unit>

  <unit name="bit"/>

  <unit name="count"/>

  <!-- Length -->
  &length;

  <!-- Area -->
  &area;

  <!-- Angle -->
  &angle;

  <!-- EM -->
  <unit name="esu" value="1*erg^0.5*centimeter^0.5"
        dimensionality="length^1.5*mass^0.5*time^-1">
    <alias>statcoulombs</alias>
    <alias>statC</alias>
    <alias>franklin</alias>
    <alias>Fr</alias>
  </unit>
  <unit name="esu_per_second" value="1*esu/second">
    <alias>statampere</alias>
  </unit>
  <unit name="ampere_turn" value="1*A"/>
  <unit name="gilbert" value="10/(4*pi)*ampere_turn"/>
  <unit name="coulomb" value="ampere*second">
    <alias>C</alias>
  </unit>
  <unit name="volt" value="joule/coulomb">
    <alias>V</alias>
  </unit>
  <unit name="farad" value="coulomb/volt">
    <alias>F</alias>
  </unit>
  <unit name="ohm" value="volt/ampere">
    <alias>Î©</alias>
  </unit>
  <unit name="siemens" value="ampere/volt">
    <alias>S</alias>
    <alias>mho</alias>
  </unit>
  <unit name="weber" value="volt*second">
    <alias>Wb</alias>
  </unit>
  <unit name="tesla" value="weber/meter^2">
    <alias>T</alias>
  </unit>
  <unit name="henry" value="weber/ampere">
    <alias>H</alias>
  </unit>
  <unit name="elementary_charge" value="1.602176487e-19*coulomb">
    <alias>e</alias>
  </unit>
  <unit name="chemical_faraday" value="9.64957e4*coulomb"/>
  <unit name="physical_faraday" value="9.65219e4*coulomb"/>
  <unit name="faraday" value="96485.3399*coulomb">
    <alias>C12_faraday</alias>
  </unit>
  <unit name="gamma" value="1e-9*tesla"/>
  <unit name="gauss" value="1e-4*tesla"/>
  <unit name="maxwell" value="1e-8*weber">
    <alias>mx</alias>
  </unit>
  <unit name="oersted" value="1000/(4*pi)*A/m">
    <alias>Oe</alias>
  </unit>
  <unit name="statfarad" value="1.112650e-12*farad">
    <alias>statF</alias>
    <alias>stF</alias>
  </unit>
  <unit name="stathenry" value="8.987554e11*henry">
    <alias>statH</alias>
    <alias>stH</alias>
  </unit>
  <unit name="statmho" value="1.112650e-12*siemens">
    <alias>statS</alias>
    <alias>stS</alias>
  </unit>
  <unit name="statohm" value="8.987554e11*ohm"/>
  <unit name="statvolt" value="2.997925e2*volt">
    <alias>statV</alias>
    <alias>stV</alias>
  </unit>
  <unit name="unit_pole" value="1.256637e-7*weber"/>
  
</ureg>
