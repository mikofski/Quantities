<?xml version="1.0"?>

<!DOCTYPE doc [
<!ENTITY prefixes SYSTEM "prefixes_en.xml">
<!ENTITY dimensions SYSTEM "dimensions_en.xml">
<!ENTITY constants SYSTEM "constants_en.xml">
]>

<ureg>

  <!-- prefixes -->
  &prefixes;

  <!-- dimensions -->
  &dimensions;

  <!-- constants -->
  &constants;

  <!-- reference -->
  <unit name="meter" dimensionality="length">
    <alias>m</alias>
    <alias>metre</alias>
  </unit>
  <unit name="second" dimensionality="time">
    <alias>s</alias>
    <alias>sec</alias>
  </unit>
  <unit name="ampere" dimensionality="current">
    <alias>A</alias>
    <alias>amp</alias>
  </unit>
  <unit name="candela" dimensionality="luminosity">
    <alias>cd</alias>
    <alias>candle</alias>
  </unit>
  <unit name="gram" dimensionality="mass">
    <alias>g</alias>
  </unit>
  <unit name="mole" dimensionality="substance">
    <alias>mol</alias>
  </unit>
  <unit name="kelvin" dimensionality="temperature">
    <alias>K</alias>
    <alias>degK</alias>
  </unit>
  <unit name="radian">
    <alias>rad</alias>
  </unit>
  <unit name="bit"/>
  <unit name="count"/>

  <!-- Angle -->
  <unit name="turn" value="2*pi*radian">
    <alias>revolution</alias>
    <alias>cycle</alias>
    <alias>circle</alias>
  </unit>
  <unit name="degree" value="pi/180*radian">
    <alias>deg</alias>
    <alias>arcdeg</alias>
    <alias>arcdegree</alias>
    <alias>angular_degree</alias>
  </unit>
  <unit name="arcminute" value="arcdeg/60">
    <alias>arcmin</alias>
    <alias>arc_minute</alias>
    <alias>angular_minute</alias>
  </unit>
  <unit name="arcsecond" value="arcmin/60">
    <alias>arcsec</alias>
    <alias>arc_second</alias>
    <alias>angular_second</alias>
  </unit>
  <unit name="steradian" value="radian^2">
    <alias>sr</alias>
  </unit>

  <!-- Area -->
  <unit name="are" value="100*m^2" dimensionality="area"/>
  <unit name="barn" value="1e-28*m^2" dimensionality="area">
    <alias>b</alias>
  </unit>
  <unit name="cmil" value="5.067075e-10*m^2" dimensionality="area">
    <alias>circular_mils</alias>
  </unit>

  <!-- Length -->
  <unit name="angstrom" value="1e-10*meter" dimensionality="length"/>
  <unit name="inch" value="2.54*centimeter" dimensionality="length">
    <alias>in</alias>
    <alias>international_inch</alias>
    <alias>inches</alias>
    <alias>international_inches</alias>
  </unit>
  <unit name="foot" value="12*inch" dimensionality="length">
    <alias>ft</alias>
    <alias>international_foot</alias>
    <alias>feet</alias>
    <alias>international_feet</alias>
  </unit>
  <unit name="mile" value="5280*foot" dimensionality="length">
    <alias>mi</alias>
    <alias>international_mile</alias>
  </unit>
  <unit name="yard" value="3*feet" dimensionality="length">
    <alias>yd</alias>
    <alias>international_yard</alias>
  </unit>
  <unit name="mil" value="inch/1000" dimensionality="length">
    <alias>thou</alias>
  </unit>
  <unit name="parsec" value="3.08568025e16*meter" dimensionality="length">
    <alias>pc</alias>
  </unit>
  <unit name="light_year" value="speed_of_light*julian_year" dimensionality="length">
    <alias>ly</alias>
    <alias>lightyear</alias>
  </unit>
  <unit name="astronomical_unit" value="149597870691*meter" dimensionality="length">
    <alias>au</alias>
  </unit>
  <unit name="nautical_mile" value="1.852e3*meter" dimensionality="length">
    <alias>nmi</alias>
  </unit>
  <unit name="printers_point" value="127*millimeter/360" dimensionality="length">
    <alias>point</alias>
  </unit>
  <unit name="printers_pica" value="12*printers_point" dimensionality="length">
    <alias>pica</alias>
  </unit>
  <unit name="US_survey_foot" value="1200*meter/3937" dimensionality="length"/>
  <unit name="US_survey_yard" value="3*US_survey_foot" dimensionality="length"/>
  <unit name="US_survey_mile" value="5280*US_survey_foot" dimensionality="length">
    <alias>US_statute_mile</alias>
  </unit>
  <unit name="rod" value="16.5*US_survey_foot" dimensionality="length">
    <alias>pole</alias>
    <alias>perch</alias>
  </unit>
  <unit name="furlong" value="660*US_survey_foot" dimensionality="length"/>
  <unit name="fathom" value="6*US_survey_foot" dimensionality="length"/>
  <unit name="chain" value="66*US_survey_foot" dimensionality="length"/>
  <unit name="barleycorn" value="inch/3" dimensionality="length"/>
  <unit name="arpentlin" value="191.835*feet" dimensionality="length"/>
  <unit name="kayser" value="1/centimeter" dimensionality="length^-1">
    <alias>wavenumber</alias>
  </unit>
  
</ureg>
