<?xml version="1.0"?>

<!DOCTYPE doc [
<!ENTITY prefixes SYSTEM "prefixes_en.xml">
<!ENTITY dimensions SYSTEM "dimensions_en.xml">
<!ENTITY constants SYSTEM "constants_en.xml">
<!ENTITY time SYSTEM "time_en.xml">
<!ENTITY length SYSTEM "length_en.xml">
<!ENTITY area SYSTEM "area_en.xml">
<!ENTITY volume SYSTEM "volume_en.xml">
<!ENTITY mass SYSTEM "mass_en.xml">
<!ENTITY force SYSTEM "force_en.xml">
<!ENTITY pressure SYSTEM "pressure_en.xml">
<!ENTITY energy SYSTEM "energy_en.xml">
<!ENTITY power SYSTEM "power_en.xml">
<!ENTITY angle SYSTEM "angle_en.xml">
<!ENTITY EM SYSTEM "EM_en.xml">
<!ENTITY frequency SYSTEM "frequency_en.xml">
]>

<ureg>

  <!-- prefixes -->
  &prefixes;

  <!-- dimensions -->
  &dimensions;

  <!-- constants -->
  &constants;

  <!-- reference -->
  <unit name="meter" dimensionality="length">
    <alias>m</alias>
    <alias>metre</alias>
  </unit>

  <unit name="second" dimensionality="time">
    <alias>s</alias>
    <alias>sec</alias>
  </unit>

  <unit name="ampere" dimensionality="current">
    <alias>A</alias>
    <alias>amp</alias>
  </unit>

  <unit name="candela" dimensionality="luminosity">
    <alias>cd</alias>
    <alias>candle</alias>
  </unit>

  <unit name="gram" dimensionality="mass">
    <alias>g</alias>
  </unit>

  <unit name="mole" dimensionality="substance">
    <alias>mol</alias>
  </unit>

  <unit name="kelvin" dimensionality="temperature">
    <alias>K</alias>
    <alias>degK</alias>
  </unit>

  <unit name="radian">
    <alias>rad</alias>
  </unit>

  <unit name="bit"/>

  <unit name="count"/>

  <!-- Time -->
  &time;

  <!-- Length -->
  &length;

  <!-- Area -->
  &area;

  <!-- Volume -->
  &volume;

  <!-- Mass -->
  &mass;

  <!-- Force -->
  &force;

  <!-- Pressure -->
  &pressure;

  <!-- Energy -->
  &energy;

  <!-- Power -->
  &power;

  <!-- Angle -->
  &angle;

  <!-- Frequency -->
  &frequency;

  <!-- EM -->
  &EM;

  <!-- Heat -->
  <!-- <unit name="RSI" value="degK*meter^2/watt"/> -->
  <!-- <unit name="clo" value="0.155*RSI"><alias>clos</alias></unit> -->
  <!-- <unit name="R_value" value="foot^2*degF*hour/btu"/> -->
  
</ureg>

# Information
byte = 8 * bit = Bo = octet
baud = bit / second = Bd = bps

# Textile
denier =  gram / (9000 * meter)
tex = gram/ (1000 * meter)
dtex = decitex

# Radiation
Bq = Hz = becquerel
curie = 3.7e10 * Bq = Ci
rutherford = 1e6*Bq = rd = Rd
Gy = joule / kilogram = gray = Sv = sievert
rem = 1e-2 * sievert
rads = 1e-2 * gray
roentgen = 2.58e-4 * coulomb / kilogram

# Temperature
degC = kelvin; offset: 273.15 = celsius
degR = 5 / 9 * kelvin; offset: 0 = rankine
degF = 5 / 9 * kelvin; offset: 255.372222 = fahrenheit

# Time

# Velocity
[speed] = [length] / [time]
knot = nautical_mile / hour = kt = knot_international = international_knot = nautical_miles_per_hour
mph = mile / hour = MPH
kph = kilometer / hour = KPH

# Viscosity
[viscosity] = [pressure] * [time]
poise = 1e-1 * Pa * second = P
stokes = 1e-4 * meter ** 2 / second = St
rhe = 10 / (Pa * s)
