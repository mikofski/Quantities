<unit name="liter" value="1e-3*m^3">
  <alias>l</alias>
  <alias>L</alias>
  <alias>litre</alias>
</unit>

<unit name="cc" value="centimeter^3">
  <alias>cubic_centimeter</alias>
</unit>

<unit name="stere" value="meter^3"/>

<unit name="gross_register_ton" value="100*foot^3">
  <alias>register_ton</alias>
  <alias>GRT</alias>
</unit>

<unit name="acre_foot" value="acre*foot">
  <alias>acre_feet</alias>
</unit>

<unit name="board_foot" value="foot^2*inch">
  <alias>FBM</alias>
</unit>

<unit name="bushel" value="2150.42*inch^3">
  <alias>bu</alias>
  <alias>US_bushel</alias>
</unit>

<unit name="dry_gallon" value="bushel/8">
  <alias>US_dry_gallon</alias>
</unit>

<unit name="dry_quart" value="dry_gallon/4">
  <alias>US_dry_quart</alias>
</unit>

<unit name="dry_pint" value="dry_quart/2">
  <alias>US_dry_pint</alias>
</unit>

<unit name="gallon" value="231*inch^3">
  <alias>liquid_gallon</alias>
  <alias>US_liquid_gallon</alias>
</unit>

<unit name="quart" value="gallon/4">
  <alias>liquid_quart</alias>
  <alias>US_liquid_quart</alias>
</unit>

<unit name="pint" value="quart/2">
  <alias>pt</alias>
  <alias>liquid_pint</alias>
  <alias>US_liquid_pint</alias>
</unit>

<unit name="cup" value="pint/2">
  <alias>liquid_cup</alias>
  <alias>US_liquid_cup</alias>
</unit>

<unit name="gill" value="cup/2">
  <alias>liquid_gill</alias>
  <alias>US_liquid_gill</alias>
</unit>

<unit name="fluid_ounce" value="gill/4">
  <alias>floz</alias>
  <alias>US_fluid_ounce</alias>
  <alias>US_liquid_ounce</alias>
</unit>

<unit name="imperial_bushel" value="36.36872*liter">
  <alias>UK_bushel</alias>
</unit>

<unit name="imperial_gallon" value="imperial_bushel/8">
  <alias>UK_gallon</alias>
</unit>

<unit name="imperial_quart" value="imperial_gallon/4">
  <alias>UK_quart</alias>
</unit>

<unit name="imperial_pint" value="imperial_quart/2">
  <alias>UK_pint</alias>
</unit>

<unit name="imperial_cup" value="imperial_pint/2">
  <alias>UK_cup</alias>
</unit>

<unit name="imperial_gill" value="imperial_cup/2">
  <alias>UK_gill</alias>
</unit>

<unit name="imperial_floz" value="imperial_gill/5">
  <alias>UK_fluid_ounce</alias>
  <alias>imperial_fluid_ounce</alias>
</unit>

<unit name="barrel" value="42*gallon">
  <alias>bbl</alias>
</unit>

<unit name="tablespoon" value="floz/2">
  <alias>tbsp</alias>
  <alias>Tbsp</alias>
  <alias>Tblsp</alias>
  <alias>tblsp</alias>
  <alias>tbs</alias>
  <alias>Tbl</alias>
</unit>

<unit name="teaspoon" value="tablespoon/3">
  <alias>tsp</alias>
</unit>

<unit name="peck" value="bushel/4">
  <alias>pk</alias>
</unit>

<unit name="fluid_dram" value="floz/8">
  <alias>fldr</alias>
  <alias>fluidram</alias>
</unit>

<unit name="firkin" value="barrel/4"/>
